{
  "name": "ls-base-lib",
  "version": "1.2.15",
  "description": "A library with no framework dependencies (e.g. vue) that encapsulates some pure native js and css. It is exported using module and umd.",
  "main": "dist/lsBaseLib.umd.js",
  "module": "dist/lsBaseLib.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackeriori/ls-base-lib"
  },
  "homepage": "https://hackeriori.github.io/ls-base-lib",
  "files": [
    "dist"
  ],
  "typings": "dist/out/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node --loader ts-node/esm build.ts",
    "buildHtml": "vite build --config vite.config.html.ts",
    "test": "vitest",
    "test:browser": "vitest --workspace=vitest.workspace.ts"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/browser": "^2.1.4",
    "axios": "^1.7.7",
    "element-plus": "^2.8.6",
    "playwright": "^1.48.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vitest-browser-vue": "^0.0.1",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-tsc": "^2.1.10"
  }
}
